{
  "name": "vscode-runrepl",
  "displayName": "Run in REPL",
  "description": "Send selected code or line to REPL in terminal",
  "version": "0.1.0",
  "publisher": "shixiongfei",
  "repository": "https://github.com/shixiongfei/vscode-runrepl",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runREPL",
        "title": "Run in REPL"
      }
    ],
    "keybindings": [
      {
        "command": "extension.runREPL.sendSelection",
        "key": "ctrl+enter",
        "when": "editorTextFocus"
      },
      {
        "command": "extension.runREPL.sendLine",
        "key": "alt+enter",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc",
    "watch": "tsc -watch"
  },
  "devDependencies": {
    "@types/node": "^20.1.2",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  }
}
